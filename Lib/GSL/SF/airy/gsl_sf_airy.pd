# PP interface to GSL

pp_addhdr('
#include <gsl/gsl_sf.h>
#include "../gslerr.h"
');

pp_def('gsl_sf_airy_Ai',
       GenericTypes => [D],
       Pars=>'double x(); double [o]y(); double [o]e()',
       Code =>'
gsl_sf_result r;
GSLERR(gsl_sf_airy_Ai_e,($x(),GSL_PREC_DOUBLE,&r))
$y() = r.val;
$e() = r.err; 
',
       Doc =>'Airy Function Ai(x).'
      );

